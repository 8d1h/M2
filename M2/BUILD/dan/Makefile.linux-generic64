# -*- makefile-gmake -*-

DEBUG ?= no
ifeq ($(DEBUG),yes)
DIRECTORY ?= linux64.debug
CONFIGOPTIONS += --enable-debug
else
DIRECTORY ?= linux64
endif
CONFIGOPTIONS += --enable-strip
CONFIGOPTIONS += --enable-tarfile
CONFIGOPTIONS += --enable-static
CONFIGOPTIONS += --with-issue=Generic
include Makefile.include
default: check
arch-chk:; [ `uname -m` = x86_64 ]

# Local Variables:
# compile-command: "time make -f Makefile.linux-generic64 rebuild-world "
# End:
