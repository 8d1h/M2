# @configure_input@
include ../include/config.Makefile
VPATH = @srcdir@

install stage2 stage1 ::; $(MAKE) -C top $@

ifeq "@M2TARFILE@" "yes"
stage2 ::; $(MAKE) -C tar stage2
endif
ifeq "@DEB@" "yes"
stage2 ::; $(MAKE) -C deb stage2
endif
ifeq "@RPM@" "yes"
stage2 ::; $(MAKE) -C rpm stage2
endif
ifeq "@DMG@" "yes"
stage2 ::; $(MAKE) -C dmg stage2
endif
ifeq "@CYGWIN@" "yes"
stage2 ::; $(MAKE) -C cygwin stage2
endif
check ::
	$(MAKE) -C deb $@
clean distclean ::
	$(MAKE) -C dmg $@
	$(MAKE) -C rpm $@
	$(MAKE) -C deb $@
	$(MAKE) -C cygwin $@
	$(MAKE) -C tar $@

uninstall install stage2 distclean ::; $(MAKE) -C install $@

Makefile : Makefile.in; cd ..; ./config.status distributions/Makefile
