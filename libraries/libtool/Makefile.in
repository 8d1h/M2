URL = ftp://ftp.gnu.org/gnu/libtool
VERSION = 1.5.24
CONFIGOPTIONS += --disable-shared
include ../Makefile.library
Makefile : @srcdir@/Makefile.in ; cd ../.. && ./config.status libraries/libtool/Makefile
clean ::
	rm -rf $(LIBRARIESDIR)/lib/libltdl*
	rm -rf $(LIBRARIESDIR)/share/aclocal/libtool.m4
	rm -rf $(LIBRARIESDIR)/share/aclocal/ltdl.m4
	rm -rf $(LIBRARIESDIR)/share/info/libtool.*
	rm -rf $(LIBRARIESDIR)/share/libtool
	rm -rf $(LIBRARIESDIR)/bin/libtool*
