# @configure_input@
include ../include/config.Makefile
VPATH = @srcdir@

install ::

ifeq "$(encap)" "yes"

install :: $(target)/encapinfo
$(target)/encapinfo: encapinfo
	$(INSTALL_DATA) $< "$@"

ifeq "$(DUMPDATA)" "yes"
install :: $(target)/postinstall
$(target)/postinstall: postinstall
	$(INSTALL_PROGRAM) $< "$@"
endif

install :: $(target)/INSTALL

$(target)/INSTALL : INSTALL
	cp $< $@

endif
