# @configure_input@
include ../../include/config.Makefile
VPATH = @srcdir@

ifeq "@CC@" "cl"
LIBEXT = lib
else
LIBEXT = a
endif

LIB  := compat.$(LIBEXT)
CFILES = $(notdir $(wildcard @srcdir@/*.c))
OFILES := $(CFILES:.c=.o)

stage1 :: $(LIB)

ifeq "@CC@" "cl"
$(LIB): $(OFILES)
	$(AR) $(ARFLAGS) /out:$@ $^	
else
$(LIB): $(OFILES)
	$(AR) $(ARFLAGS) $@ $^
	-@RANLIB@ $(LIB)
endif

distclean clean :; rm -f $(LIB) *.o
