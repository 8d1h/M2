# @configure_input@
include ../../include/config.Makefile
VPATH = @srcdir@

.PHONY : all clean distclean

all ::

install :: @encapdir@/@bindirtail@ @encapdir@/@bindirtail@/M2@EXE@

@encapdir@/@bindirtail@ :; @INSTALL@ -d "$@"

@encapdir@/@bindirtail@/M2@EXE@ : M2@EXE@; @INSTALL_PROGRAM@ M2@EXE@ @encapdir@/@bindirtail@/M2@EXE@

# install       :: @encapdir@/@bindirtail@/M2-load-libs
# @encapdir@/@bindirtail@/M2-load-libs : M2-load-libs ; @INSTALL@ $^ $@

# install       :: @encapdir@/@bindirtail@/M2-emacs
# @encapdir@/@bindirtail@/M2-emacs : M2-emacs ; @INSTALL@ $^ $@

# install       :: @encapdir@/@bindirtail@/M2-help
# @encapdir@/@bindirtail@/M2-help : M2-help ; @INSTALL@ $^ $@

# shared library links 
ifeq (@ENCAP@,yes)
ifeq (@OS@,Linux)
ifeq (@TARLIBS@,yes)
make-lib-links install :: @encapdir@/@libdirtail@; @srcdir@/liblinks M2@EXE@ @encapdir@/@libdirtail@
@encapdir@/@libdirtail@:; @INSTALL@ -d "$@"
endif
endif
endif

clean::; rm -f M2@EXE@
distclean::; rm -f Makefile
distclean:: clean
Makefile : Makefile.in; cd ../..; ./config.status Macaulay2/bin/Makefile

