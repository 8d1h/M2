# @configure_input@

VPATH = @srcdir@

# DOTS is the path to M2/Macaulay2 from this directory
DOTS = ..
include $(DOTS)/../include/config.Makefile

all check clean distclean :: ; $(MAKE) -k -C engine $@

SRCDIR = @srcdir@
include @srcdir@/Makefile.test

ifneq "$(DUMPDATA)" "no"
RESULTS += dumpdata.out
endif

all check clean distclean :: ; $(MAKE) -k -C slow $@

docStructure.out : docStructure.m2

Makefile : Makefile.in; cd $(DOTS)/..; ./config.status Macaulay2/test/Makefile

