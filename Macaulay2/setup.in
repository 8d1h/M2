#! /bin/sh -x
DUMPDATA="@DUMPDATA@"
PREFIX=../../..
M2DIR=$PREFIX/share/Macaulay2/Macaulay2/m2
BINDIR=$PREFIX/bin
cd cache
M2DIR=../$M2DIR
BINDIR=../$BINDIR
if ! test -f "$M2DIR/setup.m2"
then echo "--error: Macaulay 2 sources not found in $M2DIR" >&2
     exit 1
fi
if [ "$DUMPDATA" = yes ]
then echo Dumping data
     $BINDIR/M2 -q --stop -e 'errorDepth = 0' --dumpdata
fi
