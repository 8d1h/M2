document { 
     Key => (minors,ZZ,Matrix),
     Headline => "",
     Usage => "minors(n,M)",
     Inputs => {
		   "n" => ZZ => "order of the minor",
		   "M" => Matrix => "a map between free modules"
	  },
	Outputs => {
		   {"the ideal generated by the ", TT "n", " by ", TT "n", " minors
		   of the matrix ", TT "M", "."}
	},
	"Minors are generated in the same order as that used by ", 
	TO (subsets,ZZ,ZZ), ".",
     EXAMPLE {
	  "R = ZZ[a..f];",
	  "M = matrix{{a,b,c},{d,e,f}}",
	  "minors(2,M)",
	  "minors(2,M,Limit=>1)",
	  },

     }
document { 
     Key => [minors, Limit],
     Headline => "the maximum number of minors to compute",
     Usage => "minors(...,Limit => r)",
     Inputs => {
 	  "r" => ZZ => ""
	  },
	"the computation stops after ", TT "r", " minors are obtained. For an
	example, see ", TO (minors,ZZ,Matrix), "."
     }
